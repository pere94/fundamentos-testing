{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true,
        "bun": true,
        "node": true
    },
    "extends": "standard-with-typescript",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["jest"],
    "rules": {
      "no-unused-vars": "error",
      "no-undef": "error",
      "@typescript-eslint/no-unused-vars": "error"
    }
}
